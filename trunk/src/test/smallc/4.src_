func int bin_search(int[] ar, int start, int en, int target){
    var int m,ret end
    if (start > en)
        ret = -1
    else (
        m = (start + en) / 2;
        if (ar[m] == target)
            ret = m
        else(
            if (ar[m] < target)
                ret = bin_search(ar, m+1 , en, target)
            else
                ret = bin_search(ar, start, m-1, target)
        )
    );
    ret
}
func void main(){
    var int [] ar end
    ar = new int[10];
    ar[0] = 0;
    ar[1] = 1;
    ar[2] = 2;
    ar[3] = 3;
    ar[4] = 4;
    ar[5] = 5;
    ar[6] = 6;
    ar[7] = 7;
    ar[8] = 8;
    ar[9] = 9;
    print (bin_search(ar,0,9,3))
}