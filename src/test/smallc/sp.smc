const MIN = 2, MAX = 99;

func BaseCondition(int x, int y){
    y >= MIN && y <= MAX && y >= x
}

func S(int x, int y){
	var int [][] ar ; int s,i end
	s = x + y;
    ar = new int[][s/2 + 1 - MIN];
    i=MIN;
    while(i < s/2+1)(
        ar[i] = new int[2];
        ar[i][0] = i;
        ar[i][1] = s - i;
    );
    ar
}

func P(int x, int y){
    var int[][] ar; int i,l, p end
    p = x * y;
    i = MIN;
    l = 0;
    while(i <= p/i)
    (
        if (p%i == 0 && BaseCondition(i, p/i))
        (
             l++
        );
        i++
    );
    i = 0;
    ar = new int[][l];
    l = 0;
    while(i <= p/i)
    (
        if (p%i == 0 && BaseCondition(i, p/i))
        (
             ar[l] = new int[2];
             ar[l][0] = i;
             ar[l][1] = p/i;
